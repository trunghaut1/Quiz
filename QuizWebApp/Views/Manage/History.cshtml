@model IEnumerable<Core.Model.History>
<div class="bg-color-sky-light">
    <div class="content-md container">
        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.SubId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NumberQuest)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NumberAns)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NumberCorrect)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DateTime)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AspNetUser.Email)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.SubId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.NumberQuest)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.NumberAns)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.NumberCorrect)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DateTime)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AspNetUser.Email)
                    </td>
                    <td>
                        @*Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                            @Html.ActionLink("Delete", "Delete", new { id=item.Id })*@
                    </td>
                </tr>
            }

        </table>
    </div>
</div>
